# Block access to reprepro's internal files
<Directory "<%= documentroot %>/repos/apt/*/db/">
        Order allow,deny
        Deny from all
</Directory>

<Directory "<%= documentroot %>/repos/apt/*/conf/">
        Order allow,deny
        Deny from all
</Directory>

<Directory "<%= documentroot %>/repos/apt/*/incoming/">
        Order allow,deny
        Deny from all
</Directory>
